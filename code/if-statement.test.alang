
/// Test if statement has !=
function if_false_test(): int {
  int a = 5;
  if (a != 5) {
    return 1;
  }

  return 0;
}

/// Test if statement has ==
function if_true_test(): int {
  int a = 5;
  if (a == 5) {
    return 0;
  }

  return 1;
}

/// Test if else
function if_else_test(): int {
  int a = 5;
  if (a == 10) {
    return 1;
  } else {
    return 0;
  }

  return 1;
}

/// Test if else if else
function if_else_if_else_test(): int {
  int a = 5;
  if (a == 10) {
    return 1;
  } else if (a == 11) {
    return 10;
  } else {
    return 0;
  }

  return 1;
}